# set path to password store
basedir=~/.password-store/

# fields to be used
URL_field='URL'
USERNAME_field='UserName'

## Programs to be used
# Editor
EDITOR='gvim -f'

# Browser
BROWSER='chromium'

## Misc settings

help_color='#0C73C2'
auto_enter="'true'

# Custom Keybindings
autotype="Alt+1"
type_user="Alt+2"
type_pass="Alt+3"
open_url="Alt+4"
copy_name="Alt+c"
copy_pass="Alt+Shift+c"
edit="Alt+1"
move="Alt+2"
delete="Alt+3"
show="Alt+4"
